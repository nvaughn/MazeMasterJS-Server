{
    "defaultSeverity": "error",
    "extends": ["tslint-clean-code"],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "newspaper-order": true,
        "max-line-length": [true, 160],
        "jsdoc-format": true
    },
    "autoFixOnSave": true
}
